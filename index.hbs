{{!< default}}
{{!-- The tag above means: insert everything in this file
into the {body} of the default.hbs template --}}

<header class="site-header outer">
        {{> "post-site-nav"}}
</header>





<div class="main-feature-post">
  <div class="main-feature-inner h-100 d-flex justify-content-center align-items-center">
    <div class="main-feature-outer-frame">
    </div>
  </div>
  <div class="main-feature-words-box d-flex">
    <div class="main-feature-words-box-inner">
      <div class="row justify-content-center">
        <img class="main-feature-icon" src="https://moxieleague.nyc3.digitaloceanspaces.com/site-icons/school.svg" />
      </div>
      <p class="featurette-tag text-center highlighted-underscore">INTERVIEW</p>
      
      <h1 class="main-feature text-center">Why you are doing SEO all wrong</h1>
      <p class="main-feature">They lived near a gorgeous beach in sunny San Diego. They frequently dined at sushi restaurants. They drove a BMW.</p>
      <div class="row justify-content-center">
        <div class="readmore-outside d-flex" style="margin-top: 7.5%;">
          <div class="readmore-inside d-flex">
            <h1 class="readmore text-center align-content-center">READ MORE</h1>
          </div>
        </div>
      </div>
    </div><!-- main-feature-words-box-inner -->
  </div>
  </div>




<div class="container-fluid">
  <div class="decorative-row-first row">
      {{#get "posts" limit="3" page="1" as |posts|}}
        {{#foreach posts}}
          <div class="col-sm-4" style="padding-left: .75%; padding-right: .75%">
            {{> "moxieleague-post-card"}}
          </div>
        {{/foreach}}
      {{/get}}
  </div>
</div>


<div class="no-padding-lr-container" style="padding-left: 0; padding-right: 0;">
  <div class="second-featurette">
    {{#get "posts" limit="1" page="7" as |posts|}}
      {{#foreach posts}}
        <div class="featurette-content-row row">
          <div class="col-sm-5">
            <div class="row justify-content-center">
              <img class="featurette-tag-icon" src="https://moxieleague.nyc3.digitaloceanspaces.com/site-icons/apple.svg" />
            </div><!-- row -->

            <p class="featurette-tag text-center highlighted-underscore">INTERVIEW</p>

            <div class="justify-content-center">
              <div class="container">
                <h1 class="featurette-title text-center">Test blah glah</h1>



                <div class="featurette-excerpt">
                  <p class="featurette-excerpt">These are the best tips you'll ever need you dont need anything else. Isn't that so great?<p>
                </div><!-- featurette-excerpt -->

                <div class="readmore-outside">
                  <div class="readmore-inside">
                    <h1 class="readmore text-center">READ MORE</h1>
                  </div>
                </div>
              </div>
            </div><!-- container -->

            
          </div><!-- col-sm-5 -->
          <div class="col-sm-7 justify-content-end" style="padding-left: 0; padding-right: 0;">
            <img src= "https://moxieleague.nyc3.digitaloceanspaces.com/blog-images/EClass_Photoshop_Designing_Textures_012.jpg" />
          </div>
        </div><!-- row -->
      {{/foreach}}
    {{/get}}
  </div><!-- two-piece-feature -->
</div>

<div class="container-fluid">
  <div class="decorative-row-first row">
      {{#get "posts" limit="3" page="2" as |posts|}}
        {{#foreach posts}}
          <div class="col-sm-4" style="padding-left: .75%; padding-right: .75%">
            {{> "moxieleague-post-card"}}
          </div>
        {{/foreach}}
      {{/get}}
  </div>
</div><!-- container outer -->


{{!-- Email Sign up prompt --}}
<section class="subscribe-call-to-action">
  <div class="subscribe-content-container-outer">
    <div class="subscribe-content-container-inner">
      <div class="row justify-content-center">
        <img style="width: auto; max-height: 2.25vw; padding-bottom: 2.5%;" src="https://moxieleague.nyc3.digitaloceanspaces.com/blog-images/social-media-icons/envelope-letter-message-email-mail-3923a7717bd9f936.svg" />
      </div><!-- row -->
      <div class="row justify-content-center">
        <h3 class="footer-subscribe-form-title">SUBSCRIBE</h3>
      </div>
      <div class="row">
        <p class="footer-subscribe-form-p text-center">A newsletter about starting and building your business, delivered straight to your inbox every Wednesday AM</h5>
      </div>

      <form class="post-subscribe-form">
        <div class="row justify-content-center">
          <input class="post-subscribe-input" type="email" placeholder="Email" id="post-subscriber-email-input" required>
          <button class="btn" type="index-footer-subscribe-submit">JOIN</button>
        </div><!-- row -->
      </form>
      <div class="row justify-content-center">
        <p>Terms & Privacy</p>
      </div><!-- row -->
    </div>
  </div>
</section>


